{
  "buildCommand": "cd front-end/pet-care && npm install && npm run build",
  "outputDirectory": "front-end/pet-care/dist", 
  "framework": "vite",
  "rewrites": [
    {"source": "/((?!api|_next/static|_next/image|assets).*)", "destination": "/index.html"}
  ],
  "cleanUrls": true,
  "trailingSlash": false
}